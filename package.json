{
  "name": "koa-jungle",
  "version": "0.0.0",
  "description": "Opinionated bleeding edge koa@2 starter kit",
  "author": "Marlin Gingerich <marlin.gingerich@gmail.com>",
  "license": "MIT",
  "keywords": [
    "koa",
    "boilerplate",
    "starter",
    "launchpad",
    "es6",
    "koa-module",
    "server"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gingerich/koa-jungle.git"
  },
  "main": "src/",
  "scripts": {
    "start": "npm run serve",
    "prestart": "npm run build",
    "serve": "node --harmony dist/index.js",
    "dev": "babel-node src/bin/server.js",
    "debug": "node-debug bin/server.js",
    "build": "npm run babel && ln -s dist/bin bin",
    "watch": "npm run babel -- --watch",
    "babel": "npm run clean && babel src -d dist --source-maps inline --copy-files",
    "clean": "rm -rf dist bin",
    "test": "mocha test",
    "lint": "eslint src/**/*.js",
    "docs": "apidoc -i src/ -o docs"
  },
  "apidoc": {
    "title": "koa-jungle",
    "url": "localhost:3030"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "app-module-path": "^1.1.0",
    "joi": "^8.4.2",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.1.0",
    "koa-compress": "^2.0.0",
    "koa-conditional-get": "^1.0.3",
    "koa-convert": "^1.2.0",
    "koa-etag": "^2.1.1",
    "koa-favicon": "^2.0.0",
    "koa-helmet": "^1.0.0",
    "koa-module": "https://github.com/gingerich/koa-module.git",
    "koa-module-auth": "https://github.com/gingerich/koa-module-auth.git",
    "koa-morgan": "^1.0.1",
    "koa-mount": "^1.3.0",
    "koa-multer": "^1.0.0",
    "nconf": "^0.8.4",
    "path": "^0.12.7",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "apidoc": "^0.15.1",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.0.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015-node6": "^0.2.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-regenerator-runtime": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^2.0.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.8.1",
    "supertest": "^1.2.0"
  }
}
